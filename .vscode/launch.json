    "version": "0.2.0",
    "configurations": [
        {
            "name": "indexing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/indexing.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "app",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/app.py",
            "console": "integratedTerminal",
